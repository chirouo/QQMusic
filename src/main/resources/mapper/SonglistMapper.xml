<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qqmusic.mapper.SonglistMapper">

    <resultMap id="BaseResultMap" type="com.qqmusic.entity.Songlist">
            <id property="songlistId" column="songlist_id" jdbcType="INTEGER"/>
            <result property="songlistname" column="songlistname" jdbcType="VARCHAR"/>
            <result property="userAccount" column="user_account" jdbcType="VARCHAR"/>
            <result property="coverUrl" column="cover_url" jdbcType="VARCHAR"/>
            <result property="createDate" column="create_date" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        songlist_id,songlistname,user_account,
        cover_url,create_date
    </sql>
</mapper>
